{
  "name": "@wgr-sa/nuxt-form",
  "description": "Form builder for Nuxt",
  "version": "0.6.5",
  "repository": "https://github.com/WGR-SA/nuxt-form.git",
  "author": "jeanvier",
  "license": "MIT",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.2",
    "@types/clone": "^2.1.2",
    "class-validator": "^0.14.0",
    "maska": "^2.1.10",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.7.3",
    "changelogen": "^0.5.5",
    "eslint": "^8.49.0",
    "nuxt": "^3.7.3",
    "vitepress": "^1.0.0-beta.2"
  }
}
