{
  "name": "@wgr-sa/nuxt-form",
  "description": "Form builder for Nuxt",
  "version": "0.5.9",
  "repository": "https://github.com/WGR-SA/nuxt-form.git",
  "author": "jeanvier",
  "license": "MIT",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.1.1",
    "@nuxt/types": "^2.16.0",
    "class-validator": "^0.14.0",
    "maska": "^2.1.7",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.1",
    "@nuxt/test-utils": "^3.1.1",
    "changelogen": "^0.5.0",
    "eslint": "^8.32.0",
    "nuxt": "^3.2.2",
    "vitest": "^0.29.0"
  }
}
